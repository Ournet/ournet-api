{
  "name": "ournet-api",
  "version": "0.2.0",
  "description": "Ournet API",
  "main": "./lib/app.js",
  "private": true,
  "scripts": {
    "start": "node ./lib/app.js",
    "dev": "npm run tsc && node ./lib/app.js",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose  ./lib/*.test.js ./lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublish": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./lib/*.test.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ournet/ournet-api.git"
  },
  "keywords": [
    "ournet",
    "api"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ournet/ournet-api/issues"
  },
  "homepage": "https://github.com/Ournet/ournet-api#readme",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.3",
    "@types/lru-cache": "^4.1.1",
    "@types/mongodb": "^3.1.18",
    "@types/ms": "^0.7.30",
    "@types/node": "^10.12.18",
    "rimraf": "^2.6.2",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@ournet/domain": "^0.3.3",
    "@ournet/horoscopes-data": "^0.1.1",
    "@ournet/horoscopes-domain": "^0.1.3",
    "@ournet/images-data": "^0.1.0",
    "@ournet/images-domain": "^0.1.2",
    "@ournet/news-data": "^0.1.1",
    "@ournet/news-domain": "^0.1.1",
    "@ournet/places-data": "^0.2.2",
    "@ournet/places-domain": "^0.2.0",
    "@ournet/quotes-data": "^0.1.0",
    "@ournet/quotes-domain": "^0.1.0",
    "@ournet/topics-data": "^0.1.0",
    "@ournet/topics-domain": "^0.1.0",
    "@ournet/weather-data": "^0.2.1",
    "@ournet/weather-domain": "^0.2.2",
    "@types/express-graphql": "^0.6.2",
    "aws-sdk": "^2.382.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-jwt": "^5.3.1",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "jwks-rsa": "^1.3.0",
    "lru-cache": "^5.1.1",
    "mongodb": "^3.1.10",
    "ms": "^2.1.1",
    "news-sources": "^0.1.3",
    "object-hash": "^1.3.1",
    "ournet.logger": "^0.1.0",
    "public-holidays": "^0.2.1"
  }
}
